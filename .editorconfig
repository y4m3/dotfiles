# Repository EditorConfig
# See: https://editorconfig.org

root = true

[*]
# Default for all files
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 120
trim_trailing_whitespace = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
